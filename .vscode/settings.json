{
  "editor.tabSize": 2,
  "eslint.format.enable": true,
  "eslint.quiet": false,
  "eslint.lintTask.enable": true,
  "eslint.validate": [
    "typescript",
    "html",
    "vue",
  ],
  "stylelint.enable": true,
  "stylelint.validate": [
    "css",
    "less",
    "postcss"
  ],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[scss]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "editor.rulers": [
    80,
    120
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}